<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="SquareGeometry" class="com.albpal.pocs.testng.SquareGeometry" />
	<bean id="RectangleGeometry" class="com.albpal.pocs.testng.RectangleGeometry" />

	<bean id="Square" class="com.albpal.pocs.testng.Square" scope="prototype"/>
	<bean id="Rectangle" class="com.albpal.pocs.testng.Rectangle" scope="prototype"/>
</beans>
